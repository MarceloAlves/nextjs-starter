{
  "name": "nextjs-starter",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:staged": "yarn lint && yarn pretty:staged",
    "pretty:staged": "pretty-quick --staged",
    "pretty": "prettier --write './**/*.{js,mdx,tsx,ts,jsx}'"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "jsxSingleQuote": true
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.1",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.0",
    "framer-motion": "^5.3.0",
    "husky": "^7.0.4",
    "next": "12.0.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "eslint": "8.2.0",
    "eslint-config-next": "12.0.3",
    "typescript": "4.4.4"
  }
}
